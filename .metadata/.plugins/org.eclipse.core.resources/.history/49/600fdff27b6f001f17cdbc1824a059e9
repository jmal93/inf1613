
public class Voo {
	private Assento assentos[][];
	private int nFila;
	private int nAssentos;
	
	public Voo(int nf, int na) {
		char as;
		nFila = nf;
		nAssentos = na;
		assentos = new Assento[nf][na];
		
		for (int i = 0; i < nf; i++) {
			for (char j = 'A'; j < ('A' + na); j++) {
				assentos[i][j] = new Assento(i + 1, (char)j);
				System.out.printf("%d %c\n", i + 1, (char)j);
			}
		}
	}
}
