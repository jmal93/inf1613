
public class Socio {
	private String nome;
	private String endereco;
	private String dtNasc;
	private int matric;

	public Socio(int x) {
		matric = x;
	}
	
	public String getMatric() {
		double formatado = matric % 10;
		System.out.println(formatado);
		return Integer.toBinaryString(formatado);
	}
}
